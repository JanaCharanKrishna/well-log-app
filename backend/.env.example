# Database
DATABASE_URL=postgresql://welllog:welllog123@localhost:5432/welllog_db

# AWS S3 (optional — leave empty to skip S3 uploads)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=well-log-files

# AI Provider — set at least one key for AI interpretation and chat
# Groq is free: https://console.groq.com/keys
GROQ_API_KEY=
# OpenAI (fallback if Groq is not set)
OPENAI_API_KEY=

# CORS — comma-separated list of allowed origins
CORS_ORIGINS=http://localhost

# Upload
UPLOAD_DIR=/tmp/welllog_uploads
