# ── Docker Compose Environment Variables ──

# Database credentials (used by both the db and backend containers)
POSTGRES_USER=welllog
POSTGRES_PASSWORD=welllog123
POSTGRES_DB=welllog_db

# Port mapping (host ports)
DB_PORT=5432
BACKEND_PORT=8000
FRONTEND_PORT=80

# CORS — if you are hosting behind a domain, set this to your domain
CORS_ORIGINS=http://localhost
